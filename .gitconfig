[user]
	name = exTerEX
	email = 30706568+exTerEX@users.noreply.github.com
[init]
	defaultBranch = main
[github]
	user = exterex
[color]
	ui = auto
[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold
[status]
	submoduleSummary = true
[diff]
	submodule = log
[tag]
	forcesignannotated = true
[help]
	autocorrect = 1
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[submodule]
	recurse = true
[credential]
	helper = cache